{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@yext/answers-headless-react": ["src"]
    },
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "react-app.d.ts"
  ]
}